#:kivy
<MyRoot>

    text_field: text_field

    orientation: 'vertical'
    TextInput:
        id: text_field
        height 200
        size_hint: (1, None)
        readonly: True
        disabled: True
        font_size: 64
    GridLayout:
        rows: 2
        columns: 4
        padding: 10
        spacing: 10
        height: 400
        size_hint: (1, None)
        Button:
            text: "1"
            font_size: 26
            on_press: root.app_symbol('1')
        Button:
            text: "2"
            font_size: 26
            on_press: root.app_symbol('2')
        Button:
            text: "3"
            font_size: 26
            on_press: root.app_symbol('3')
        Button:
            text: "4"
            font_size: 26
            on_press: root.app_symbol('4')
        Button:
            text: "5"
            font_size: 26
            on_press: root.app_symbol('5')
        Button:
            text: "6"
            font_size: 26
            on_press: root.app_symbol('5')
        Button:
            text: "7"
            font_size: 26
            on_press: root.app_symbol('5')
        Button:
            text: "8"
            font_size: 26
            on_press: root.app_symbol('5')
